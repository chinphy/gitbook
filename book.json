{
    "title": "Chinphy's Tour",
    "author": "Chinphy",
    "description": "study tour",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "plugins": [
        "-highlight",
        "theme-comscore",
        "github",
        "splitter",
        "tbfed-pagefooter",
        "toggle-chapters",
        "prism"
    ],
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/chinphy"
        },
        "tbfed-pagefooter": {
            "copyright":"Copyright © chinphy.com 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "prism": {
            "css": [
              "prismjs/themes/prism-okaidia.css"
            ]
        }
    }
}